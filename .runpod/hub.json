{
  "title": "ComfyUI RunPod Template",
  "description": "ComfyUI exposed on port 8188, with auto-updates and preinstalled nodes/models. Based on CUDA 12.8+.",
  "type": "serverless",
  "category": "image",
  "iconUrl": "",
  "config": {
    "runsOn": "GPU",
    "containerDiskInGb": 20,
    "gpuCount": 1,
    "allowedCudaVersions": ["12.8", "12.7", "12.6"],
    "presets": [
      {
        "name": "Default",
        "env": {
          "COMFY_PORT": "8188"
        }
      }
    ]
  }
}
