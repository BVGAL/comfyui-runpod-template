{
  "title": "ComfyUI RunPod Template",
  "description": "ComfyUI exposed on port 8188, with auto-updates, preinstalled nodes, and required models. Based on CUDA 12.8+.",
  "type": "serverless",
  "category": "image",
  "iconUrl": "",
  "config": {
    "runsOn": "GPU",
    "containerDiskInGb": 20,
    "gpuCount": 1,
    "allowedCudaVersions": ["12.4", "12.5", "12.6", "12.8", "12.2"]
    "presets": [
      {
        "name": "Default",
        "env": {
          "COMFY_PORT": "8188"
        }
      }
    ]
  }
}

